[
  {
    "Name": "any",
    "Description": "Checks if any of the provided arguments evaluate to true (Logical OR). Returns false if no arguments are true.",
    "Examples": [
      "any(true, false)",
      "any(1 == 2, 3 > 1)",
      "any($boolVar, false)"
    ],
    "Arguments": [
